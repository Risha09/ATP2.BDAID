<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/respond.js"></script>
    <link href="~/Content/slateTheme.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
</head>
<body style="padding: 0" ng-app="bdApp">
    <div class="container-fluid body-content">
        @RenderBody()
    </div>

    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/Chart.js"></script>
    <script src="~/Scripts/angular-chart.js"></script>
    <script src="~/Scripts/controller/app.js"></script>
    <script src="~/Scripts/controller/AllController.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
